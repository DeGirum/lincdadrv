#!/usr/bin/make -f
# See debhelper(7) (uncomment to enable)
# output every command that modifies files on the build system.
#export DH_VERBOSE = 1

include /usr/share/dpkg/pkg-info.mk

clean:
	$(MAKE) clean

build:
	-$(MAKE) dkms-clean
	$(MAKE) dkms

install:

build-arch:

build-indep:

binary:

binary-arch:

binary-indep:

